<% @users.each do |user| %>
<li>
	<%= gravatar_for user %>
	<%= link_to user.username, user %>
	<% if current_user.admin? && !current_user?(user) %>
	<button type="button" class="btn btn-danger pull-right"><%= link_to "delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %></button>
	<% end %>
</li>
<% end %>